{
    if ( $rspamd{'SpamCheckStatus'} eq 'enabled') {
        $OUT .= "\n -- symbols/lua rules are not disabled\n";
    } else {
        # Disable all lua rules
        $OUT .= "-- disable symbol rules\n";

        my @symbols = qw(
            SPOOF_REPLYTO
        );
        foreach (@symbols) {
            $OUT .= "rspamd_config:add_condition('$_', function(task) return false end)";
        }
    }
}
